Algoritmo "semnome"

Var
     num, a, b, c, mult:inteiro

Inicio
   escreva("Digite o Numero: ")
   leia(num)

   a <- 0
   mult <- 0
   c <- 0

   enquanto (a < 10) faca

      a <- a + 1
      c <- c + 1
      mult <- mult + 1
      b <- num * mult

      escreval(num," x",mult,"=",b)

   fimenquanto


Fimalgoritmo